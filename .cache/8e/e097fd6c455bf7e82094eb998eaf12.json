px;\r\n    height: auto;\r\n    border-radius: 50px;\r\n    color: #000000;\r\n    background: transparent;\r\n    box-shadow: inset 20px 20px 70px #bebebe,\r\n        inset -20px -20px 70px #ffffff;\r\n    ;\r\n    padding: 10px;\r\n    text-align: center;\r\n    max-height: 60vh;\r\n    overflow-y: auto;\r\n    z-index: 1;\r\n}\r\n\r\n.completed {\r\n    position: relative;\r\n    top: 30px;\r\n    padding-top: 25px;\r\n    box-sizing: border-box;\r\n    height: auto;\r\n    /* overflow: hidden;\r\n    overflow-y: auto; */\r\n    user-select: none;\r\n    /* z-index: 10; */\r\n    padding: 10px;\r\n    color: #000000;\r\n    text-align: center;\r\n    border-radius: 50px;\r\n    background: transparent;\r\n    box-shadow: inset 20px 20px 70px #bebebe,\r\n        inset -20px -20px 70px #ffffff;\r\n}\r\n\r\n.item {\r\n    position: relative;\r\n    height: 70px;\r\n    width: 783px;\r\n    margin-top: 4px;\r\n    border-radius: 20px;\r\n    overflow: auto;\r\n    box-sizing: border-box;\r\n    z-index: 0;\r\n}\r\n\r\n.task {\r\n    position: absolute;\r\n    background: transparent;\r\n    font-size: 20px;\r\n    top: 20px;\r\n    left: 20px;\r\n    max-width: 70%;\r\n    overflow-wrap: break-word;\r\n    text-shadow: 1px 2px 0px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.high {\r\n    background-color: rgb(231, 63, 63);\r\n    color: #fff;\r\n    border-radius: 10px;\r\n}\r\n\r\n.normal {\r\n    background-color: rgb(137, 227, 112);\r\n    border-radius: 10px;\r\n}\r\n\r\n.low {\r\n    background-color: rgb(255, 255, 255);\r\n    color: #000000;\r\n    margin-right: 20px;\r\n    border-radius: 10px;\r\n}\r\n\r\n.priority-control {\r\n    position: absolute;\r\n    background: rgba(11, 0, 0, 0.35);\r\n    box-shadow: 0 8px 32px 0 rgba(31, 38, 135, 0.37);\r\n    backdrop-filter: blur(20px);\r\n    -webkit-backdrop-filter: blur(20px);\r\n    border-radius: 10px;\r\n    border: 1px solid rgba(255, 255, 255, 0.18);\r\n    height: 69px;\r\n    width: 100px;\r\n    right: 80px;\r\n    top: 0px;\r\n}\r\n\r\n.priority-control>span {\r\n    position: relative;\r\n    top: 24px;\r\n    left: 12px;\r\n    width: 20px;\r\n    height: 20px;\r\n    display: inline-block;\r\n    border: 1px solid white;\r\n    cursor: pointer;\r\n}\r\n\r\n.remove-btn {\r\n    position: absolute;\r\n    background-color: rgb(27, 1, 1);\r\n    height: 70px;\r\n    width: 78px;\r\n    right: 0px;\r\n    top: 0px;\r\n    font-size: 12px;\r\n    padding-top: 27px;\r\n    text-align: center;\r\n    box-sizing: border-box;\r\n    cursor: pointer;\r\n    user-select: none;\r\n    color: white;\r\n    border-radius: 25px;\r\n}\r\n\r\n\r\n.completed>* {\r\n    opacity: 0.3;\r\n    user-select: none;\r\n}\r\n\r\n\r\n#clear-completed {\r\n    position: relative;\r\n    top: 2px;\r\n    margin-bottom: 2px;\r\n    border-radius: 20px;\r\n    color: #40ac01;\r\n    font-size: 14px;\r\n    font-weight: bold;\r\n    text-align: center;\r\n    background: transparent;\r\n    padding: 10px;\r\n}"},"lineCount":212}},"error":null,"hash":"f7e2f04e9b7018731173d1e7cae281c5","cacheData":{"env":{}}}